{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "lint:scss": "stylelint \"projects/**/*.scss\" --fix",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --baseHref=/extensions/",
    "predeploy": "npm run build:docs",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "build:lib": "ng build extensions --configuration production",
    "build:moment-adapter": "ng build extensions-moment-adapter --configuration production",
    "copy:scss": "gulp --gulpfile gulpfile.js",
    "publish": "npm run build:lib && npm run copy:scss && cd dist/extensions && npm publish --tag v12-lts",
    "publish:moment-adapter": "npm run build:moment-adapter && cd dist/extensions-moment-adapter && npm publish --tag v12-lts",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.16",
    "@angular/localize": "~12.2.16",
    "@angular/material": "~12.2.13",
    "@angular/material-moment-adapter": "~12.2.13",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^7.4.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "moment": "^2.29.1",
    "ngx-color": "~7.2.0",
    "ngx-progressbar": "^6.1.1",
    "photoviewer": "^3.6.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-eslint/builder": "^12.7.0",
    "@angular-eslint/eslint-plugin": "^12.7.0",
    "@angular-eslint/eslint-plugin-template": "^12.7.0",
    "@angular-eslint/schematics": "^12.7.0",
    "@angular-eslint/template-parser": "^12.7.0",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "angular-cli-ghpages": "^1.0.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.3.1",
    "husky": "^7.0.4",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "lint-staged": "^11.2.6",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^12.2.7",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-node": "~10.0.0",
    "tsickle": "^0.43.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
